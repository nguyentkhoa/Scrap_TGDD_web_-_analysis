    "# \n",
    "class StartBrowser():\n",
    "    def __init__(self, url):\n",
    "        self.url = url\n",
    "        self.driver = None\n",
    "    def start_browser(self):\n",
    "        # Set up the Chrome options\n",
    "        self.driver = webdriver.Chrome()\n",
    "        self.driver.get(self.url)\n",
    "        self.driver.implicitly_wait(10)\n",
    "    def close_browser(self):\n",
    "        if self.driver:\n",
    "            self.driver.quit()\n",
    "            self.driver = None\n",
    "    def run(self):\n",
    "        self.start_browser()\n",
    "        self.driver.implicitly_wait(10)\n",
    "        print(\"Browser started and URL opened.\")\n",
    "class Click_Filter():\n",
    "    def __init__(self, driver):\n",
    "        self.driver = driver\n",
    "    def try_close_ads(self):\n",
    "        for i in range(3):\n",
    "            try:\n",
    "                close_ads=self.driver.find_element(By.CLASS_NAME,'icon-close-popup').click()\n",
    "            except:\n",
    "                print(\"Close ads for the {} time\".format(i+1))\n",
    "                i+=1\n",
    "                sleep(0.5)\n",
    "    def click_phone_category(self):\n",
    "        main_menu=self.driver.find_element(By.XPATH,'//ul[@class=\"main-menu\"]')\n",
    "        category_phone=main_menu.find_elements(By.TAG_NAME, \"span\")[0].click()\n",
    "        print('Click to phone category')\n",
    "        sleep(2)\n",
    "    def click_phone_brand(self):\n",
    "        filter=self.driver.find_element(By.CLASS_NAME,'filter-total ').click()\n",
    "        sleep(2)\n",
    "        filter_menu=self.driver.find_element(By.CLASS_NAME,'filter-list')\n",
    "        each_phone_filter=filter_menu.find_elements(By.CSS_SELECTOR, \"a.c-btnbox.filter-manu\")\n",
    "        for i in each_phone_filter:\n",
    "            i.click()\n",
    "            sleep(0.3)\n",
    "        sleep(2)\n",
    "        self.driver.implicitly_wait(10)\n",
    "        show_product=self.driver.find_element(By.XPATH,'//a[@class=\"btn-filter-readmore\"]').click()\n",
    "        sleep(2)\n",
    "        try:\n",
    "            best_sell_filter=self.driver.find_element(By.XPATH,'//a[text()=\"Bán chạy\"]').click()\n",
    "        except:\n",
    "            print(\"Best sell filter not found\")\n",
    "        sleep(2)\n",
    "    def show_all_product(self):\n",
    "        while True:\n",
    "            try:\n",
    "                show_more_product=self.driver.find_element(By.CLASS_NAME,'view-more').click()\n",
    "                self.driver.implicitly_wait(10)\n",
    "                sleep(2)\n",
    "            except:\n",
    "                print('No more product')\n",
    "                sleep(2)\n",
    "                break\n",
    "    def run(self):\n",
    "        self.try_close_ads()\n",
    "        self.click_phone_category()\n",
    "        self.click_phone_brand()\n",
    "        self.show_all_product()\n",
    "        print(\"All products are shown.\")\n",
    "class Crawl_Product():\n",
    "    def __init__(self, driver):\n",
    "        self.driver = driver\n",
    "    def get_info(self, product):\n",
    "        phone_id=product.find_element(By.TAG_NAME,'a').get_attribute('data-id')\n",
    "        phone_name=product.find_element(By.TAG_NAME,'a').get_attribute('data-name')\n",
    "        phone_price=product.find_element(By.TAG_NAME,'a').get_attribute('data-price')\n",
    "        phone_brand=product.find_element(By.TAG_NAME,'a').get_attribute('data-brand')\n",
    "        category=product.find_element(By.TAG_NAME,'a').get_attribute('data-cate')\n",
    "        phone_img=product.find_element(By.TAG_NAME,'img').get_attribute('src')\n",
    "        try:\n",
    "            phone_screen=product.find_element(By.CLASS_NAME,'item-compare.gray-bg').get_attribute('textContent')\n",
    "        except:\n",
    "            phone_screen='None'\n",
    "        try:\n",
    "            phone_utility=product.find_element(By.CLASS_NAME,'utility').get_attribute('textContent')\n",
    "        except:\n",
    "            phone_utility='None'\n",
    "        try:\n",
    "            phone_rating=product.find_element(By.CLASS_NAME,'rating_Compare').get_attribute('textContent')\n",
    "        except:\n",
    "            phone_rating='None'\n",
    "\n",
    "        result={\n",
    "            'phone_id':phone_id,\n",
    "            'phone_name':phone_name,\n",
    "            'phone_price':phone_price,\n",
    "            'phone_brand':phone_brand,\n",
    "            'category':category,\n",
    "            'phone_img':phone_img,\n",
    "            'phone_screen':phone_screen,\n",
    "            'phone_utility':phone_utility,\n",
    "            'phone_rating':phone_rating\n",
    "        }\n",
    "        print(phone_name)\n",
    "        return result\n",
    "    def crawl_all_product(self):\n",
    "        list_phone_elm=self.driver.find_element(By.XPATH,'//ul[@class=\"listproduct\"]')\n",
    "        product_detail=list_phone_elm.find_elements(By.CSS_SELECTOR,'li.item.ajaxed.__cate_42')\n",
    "        all_product=[self.get_info(product) for product in product_detail]\n",
    "        print(\"All products are crawled.\")\n",
    "        return all_product"
   ]
